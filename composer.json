{
    "name": "openpsa/openpsa2.org",
    "minimum-stability": "dev",
    "require": {
        "openpsa/midcom": "dev-master",
        "openpsa/midcom-console": "dev-master",
        "openpsa/midgard-portable": "dev-master"
    },
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "openpsa\\installer\\installer::setup_root_package"
        ],
        "post-update-cmd": [
            "openpsa\\installer\\installer::setup_root_package"
        ]
    },
    "extra": {
        "midcom-static-dir": "midcom-static"
    }
}
